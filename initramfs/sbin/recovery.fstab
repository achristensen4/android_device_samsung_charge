# mount     fstype  device                  [device2]   [fstype2]   [fsoptions]         [fsoptions2]

/boot       mtd     boot
/cache      ext4    /dev/block/mmcblk0p3        NULL        rfs         NULL            nosuid nodev check=no
/data       ext4    /dev/block/mmcblk0p1        NULL        rfs         NULL            nosuid nodev check=no
/datadata   ext4    /dev/block/stl11            NULL        rfs         NULL            nosuid nodev check=no
/recovery   mtd     recovery
/sdcard     vfat    /dev/block/mmcblk1p1
/system     ext4    /dev/block/stl10            NULL        rfs         NULL            llw,check=no
